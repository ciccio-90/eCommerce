@model IEnumerable<eCommerce.Storefront.Services.ViewModels.CategoryView>

<h2>Categories</h2>
<ul class="refine-attributes">
@foreach (var categoryView in Model)
{
    <li>@Html.ActionLink(categoryView.Name, "GetProductsByCategory", "Product", new { categoryId = categoryView.Id }, null)</li>
}
</ul>